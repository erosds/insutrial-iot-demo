{
  "dashboard": {
    "id": null,
    "title": "Industrial IoT Monitoring Dashboard",
    "tags": ["industrial", "iot", "sensors"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Real-time Sensor Values",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT sensor_type, value, unit FROM sensor_data WHERE time >= now() - interval '5 minutes' ORDER BY time DESC LIMIT 3",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "Temperature Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT time, value FROM sensor_data WHERE sensor_type = 'temperature' AND time >= now() - interval '1 hour' ORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 40
                }
              ]
            },
            "unit": "celsius"
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 3,
        "title": "Pressure Monitoring",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT time, value FROM sensor_data WHERE sensor_type = 'pressure' AND time >= now() - interval '1 hour' ORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1.8
                },
                {
                  "color": "red",
                  "value": 2.0
                }
              ]
            },
            "unit": "pressurebars"
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 4,
        "title": "Vibration Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT time, value FROM sensor_data WHERE sensor_type = 'vibration' AND time >= now() - interval '1 hour' ORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1.5
                },
                {
                  "color": "red",
                  "value": 2.0
                }
              ]
            },
            "unit": "velocityms"
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": []
          }
        }
      },
      {
        "id": 5,
        "title": "Sensor Quality Overview",
        "type": "table",
        "targets": [
          {
            "expr": "SELECT sensor_type, AVG(quality) as avg_quality, MIN(quality) as min_quality, MAX(quality) as max_quality FROM sensor_data WHERE time >= now() - interval '15 minutes' GROUP BY sensor_type ORDER BY sensor_type",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": false,
              "displayName": "sensor_type"
            }
          ]
        }
      },
      {
        "id": 6,
        "title": "System Health Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT COUNT(*) as total_records FROM sensor_data WHERE time >= now() - interval '1 hour'",
            "refId": "A"
          },
          {
            "expr": "SELECT COUNT(DISTINCT machine_id) as active_machines FROM sensor_data WHERE time >= now() - interval '5 minutes'",
            "refId": "B"
          },
          {
            "expr": "SELECT COUNT(*) as anomalies FROM sensor_data WHERE sensor_type = 'anomaly' AND time >= now() - interval '1 hour'",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 7,
        "title": "Historical Data Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT time_bucket('5 minutes', time) as time_bucket, sensor_type, AVG(value) as avg_value FROM sensor_data WHERE time >= now() - interval '4 hours' GROUP BY time_bucket, sensor_type ORDER BY time_bucket",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 5,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": [
              "mean",
              "max",
              "min"
            ]
          }
        }
      },
      {
        "id": 8,
        "title": "Anomaly Detection Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "SELECT time, status as anomaly_type FROM sensor_data WHERE sensor_type = 'anomaly' AND time >= now() - interval '24 hours' ORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "points",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "always",
              "pointSize": 8,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "Anomaly Events",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "fixed",
              "fixedColor": "red"
            },
            "mappings": [
              {
                "options": {
                  "OUT_OF_RANGE": {
                    "color": "orange",
                    "index": 0
                  },
                  "HIGH_VIBRATION": {
                    "color": "red",
                    "index": 1
                  },
                  "LOW_QUALITY": {
                    "color": "yellow",
                    "index": 2
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom",
            "calcs": []
          }
        }
      }
    ],
